<!DOCTYPE html>
<html lang="en">

<head>
    <title>Profile page</title>
</head>

<body>
    <%- include('common/header')%>
    <h1>Welcome <%= user.name %>
    </h1>
    <h3>Email: <%= user.email%>
    </h3>
    <h4>City: <%= user.city%>
    </h4>

    <ul>
        <% user.skills.forEach((item)=>{ %>
            <li>
                <%= item %>
            </li>
            <% }) %>
    </ul>

</body>

</html>